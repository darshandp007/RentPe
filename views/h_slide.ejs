<img class="mySlides" src="/images/h7.jpg" height="" width="100%" >


<script>
var a=0;
showdisp();

function showdisp(){
    var x=document.getElementsByClassName("mySlides");
    for(i=0;i<x.length;i++){
        x[i].style.display="none";
    }
    a++;
    if(a>x.length){
        a=1;
    }
    x[a-1].style.display="block";
    setTimeout(showdisp,2000);
}
</script>
